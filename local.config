params.run_type='local'
params.project_folder='/Users/jboucas/nf-kallisto-deseq2-test/'
params.raw_data='/Users/jboucas/Desktop/test_ftp/'

params.fastqc_cpus = 2

docker.enabled = true
process {
    withName:fastqc {
        cpus=8
        container = 'mpgagebioinformatics/fastqc:0.11.9'
        containerOptions = "-v ${params.project_folder}:/workdir -v ${params.raw_data}:/raw_data"
    }
}

